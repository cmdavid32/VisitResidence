<?xml version='1.0' encoding='utf-8'?>
<widget id="com.techconsultores.visitresidence" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
  <name>VisitResidence</name>
  <description>
    Aplicación para creación de visitias.
  </description>
  <author email="info@apprepuestos.com" href="http://visitresidence.com">
    Tech Consultores
  </author>
  <content src="index.html" />
  <access origin="*" />
  <plugin name="cordova-plugin-whitelist" version="1" />
  <allow-intent href="http://*/*" />
  <allow-intent href="https://*/*" />
  <allow-intent href="tel:*" />
  <allow-intent href="sms:*" />
  <allow-intent href="mailto:*" />
  <allow-intent href="geo:*" />
  <platform name="android">
    <allow-intent href="market:*" />
  </platform>
  <platform name="ios">
    <allow-intent href="itms:*" />
    <allow-intent href="itms-apps:*" />
  </platform>
  <feature name="Notification">
    <param name="android-package" value="org.apache.cordova.dialogs.Notification" />
  </feature>
  <platform name="android">
    <icon qualifier="ldpi" src="res/icons/android/Icon-96.png" />
    <icon qualifier="mdpi" src="res/icons/android/Icon-48.png" />
    <icon qualifier="hdpi" src="res/icons/android/Icon-72.png" />
    <icon qualifier="xhdpi" src="res/icons/android/Icon-96.png" />
    <icon qualifier="xxhdpi" src="res/icons/android/Icon-144.png" />
    <icon qualifier="xxxhdpi" src="res/icons/android/Icon-192.png" />
  </platform>
  <engine name="android" spec="^7.1.4" />
</widget>
